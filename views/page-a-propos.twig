{% extends "templates/common/base.twig" %}
{% set template = 'a-propos' %}

{# Variables disponibles :
	{{ dump(hero) }}
	{{ dump(groupe) }}
	{{ dump(equipe) }}
	{{ dump(archives) }}
 #}


{% block main %}
	<div class="container post-type-{{post.post_type}}" id="post-{{post.ID}}">

		BIENVENUE SUR LA PAGE A PROPOS
		
		<h1>HERO</h1>
		<p>{{ hero.titre }}</p>
		<p>{{ hero.texte }}</p>
		<img src="{{ hero.img.url }}" alt="{{ hero.img.alt }}">

		<h1>BANNIERE</h1>
		<img src="{{ groupe.url }}" alt="{{ groupe.alt }}">

		<h1>EQUIPE</h1>
		<p>{{ equipe.titre }}</p>
		<p>{{ equipe.texte }}</p>
		<p>{{ equipe.btn.label }}</p>
		<p>{{ equipe.btn.url }}</p>

		<h1>ARCHIVES</h1>
		<p>{{ fichiers.titre }}</p>
		<p>{{ fichiers.texte }}</p>
		<p>{{ fichiers.embed }}</p>

	</div>
{% endblock %}
